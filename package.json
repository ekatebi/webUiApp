    "eslint": "eslint ./src",
    "esw": "esw -w ./src",
    "test": "karma start",
    "single": "npm test -- --single-run true",
    "project": "node install/project.js",
    "clean": "git clean -Xfd",
    "realclean": "git clean -xfd",
    "prebuild": "npm install",
    "build": "npm run dist",
    "xtar": "tar cvzf misc/app.tgz ../app --exclude-from=.gitignore",
    "tar": "rm -f ../app.tar.gz && tar -zcvf ../app.tar.gz ../app --exclude ../app/node_modules --exclude ../app/backend/node_modules --exclude ../app/backend/build",
    "rmod": "rm -rf node_modules/ && npm cache clean",
    "rmodb": "rm -rf node_modules/ && npm cache clean && npm install && npm start",
    "s-wrap": "npm shrinkwrap"
